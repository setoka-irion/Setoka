<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.practice.setoka.mapper.TestMapper">
  
  	<insert id="insertContent" >
  		INSERT INTO testtable (content) VALUES (#{content})
  	</insert>
  	
  	<select id="selectContent">
    	SELECT content FROM testtable WHERE num=#{num} 
  	</select>
  </mapper>
